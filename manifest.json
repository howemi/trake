{
    "manifest_version": 3,
    "name": "Trake",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_title": "Website Usage",
        "default_icon": {
            "16": "/images/favicon-16x16.png",
            "32": "/images/favicon-32x32.png",
            "192": "/images/android-chrome-192x192.png"
        }
    },
    "icons": {
        "16": "/images/favicon-16x16.png",
        "32": "/images/favicon-32x32.png",
        "192": "/images/android-chrome-192x192.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "description": "Personal website usage tracker",
    "author": "Michael Howe"
}
